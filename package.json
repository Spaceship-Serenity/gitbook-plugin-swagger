{
  "name": "gitbook-plugin-swagger",
  "version": "0.1.0",
  "description": "A plugin to consume Swagger files into a Gitbook",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gnomeontherun/gitbook-plugin-swagger.git"
  },
  "keywords": [
    "gitbook",
    "plugin"
  ],
  "author": "Jeremy Wilken <gnomation@gnomeontherun.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gnomeontherun/gitbook-plugin-swagger/issues"
  },
  "homepage": "https://github.com/gnomeontherun/gitbook-plugin-swagger#readme",
  "engines": {
    "gitbook": ">=3"
  },
  "gitbook": {
    "properties": {
      "file": {
        "type": "string",
        "default": "",
        "description": "Relative path to snippets file to use."
      }
    }
  },
  "dependencies": {
    "bootprint": "^0.8.5",
    "bootprint-swagger": "^0.13.1"
  }
}
